<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root alignment="CENTER" maxWidth="-Infinity" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="EDIT SUPPLIER">
               <font>
                  <Font name="Koulen Regular" size="25.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Text>
            <Region HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Region>
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseButtonClicked" text="Close">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Koulen Regular" size="12.0" />
               </font></Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
            <VBox>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="NAME" textAlignment="CENTER">
                     <font>
                        <Font name="Koulen Regular" size="15.0" />
                     </font>
                  </Text>
                  <Text fx:id="businessNameText" fill="#8a8787" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER">
                     <font>
                        <Font name="Koulen Regular" size="15.0" />
                     </font>
                  </Text>
               </children>
               <padding>
                  <Insets top="23.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Information">
         <font>
            <Font name="Koulen Regular" size="25.0" />
         </font>
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="10.0" />
         </VBox.margin>
      </Text>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" spacing="14.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Industry">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="VAT Number">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Phone number">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Payment methods">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
               </children>
            </VBox>
            <VBox spacing="4.0">
               <children>
                  <TextField fx:id="nameText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="industryText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="vatNumberText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="emailText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="phoneText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <Text fx:id="paymentMethodsText" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="8.0" />
                     </VBox.margin></Text>
               </children>
            </VBox>
            <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="70.0" spacing="14.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Street">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Street nr">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="City">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Zipcode">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Country">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></Text>
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="4.0">
               <children>
                  <TextField fx:id="streetText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="streetNrText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="cityText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="zipcodeText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="countryText">
                     <font>
                        <Font name="Koulen Regular" size="12.0" />
                     </font></TextField>
                  <Button fx:id="deactiveButton" mnemonicParsing="false" onAction="#onDeactiveButtonClicked" text="Deactive account" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="15.0">
         <children>
            <Button fx:id="buttonEditOrSubmit" alignment="CENTER" mnemonicParsing="false" onAction="#onEditOrSubmitClicked" textAlignment="CENTER" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelClicked" text="CANCEL" />
         </children>
      </HBox>
      <Label fx:id="errorLabel" textFill="#ec4842" />
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</fx:root>
