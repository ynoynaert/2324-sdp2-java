<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root alignment="TOP_CENTER" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="245.0" style="-fx-background-color: #d3d3d3;" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="TOP_CENTER">
            <children>
                <ImageView fitHeight="84.0" fitWidth="237.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/logo_delaware.png" />
                    </image>
                </ImageView>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, supplier!">
               <font>
                  <Font name="Koulen Regular" size="18.0" />
               </font>
            </Text>
            </children>
         <VBox.margin>
            <Insets bottom="75.0" />
         </VBox.margin>
        </VBox>
        <VBox alignment="CENTER" prefHeight="344.0" prefWidth="375.0" spacing="40.0">
            <children>
            <Button fx:id="firstSelectionButton" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onOrdersClicked" prefHeight="1.0" prefWidth="1.0">
               <font>
                  <Font name="Koulen Regular" size="20.0" />
               </font>
            </Button>
                <Button fx:id="ordersButton" alignment="CENTER" contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onOrdersClicked" prefWidth="175.0" text="ORDERS">
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Koulen Regular" size="20.0" />
                    </font>
                </Button>
                <Button fx:id="clientButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onClientsClicked" prefWidth="175.0" text="MY CLIENTS">
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Koulen Regular" size="20.0" />
                    </font>
                </Button>
                <Button fx:id="accountButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onAccountClicked" prefWidth="175.0" text="MY ACCOUNT">
                    <font>
                        <Font name="Koulen Regular" size="20.0" />
                    </font>
                </Button>
            </children>
        </VBox>
        <VBox alignment="BOTTOM_CENTER" prefHeight="350.0" prefWidth="375.0">
            <children>
                <Button fx:id="logOutButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onLogoutClicked" prefWidth="175.0" text="LOG OUT">
                    <font>
                        <Font name="Koulen Regular" size="20.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                    </VBox.margin>
                </Button>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </VBox>
    </children>
</fx:root>
